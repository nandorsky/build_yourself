<p id="notice"><%= notice %></p>

<h1>Listing Quartergoals</h1>

<table>
  <thead>
    <tr>
      <th>Goal</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @quartergoals.each do |quartergoal| %>
      <tr>
        <td><%= quartergoal.goal %></td>
        <td><%= quartergoal.user_id %></td>
        <td><%= link_to 'Show', quartergoal %></td>
        <td><%= link_to 'Edit', edit_quartergoal_path(quartergoal) %></td>
        <td><%= link_to 'Destroy', quartergoal, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Quartergoal', new_quartergoal_path %>
